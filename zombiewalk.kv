#:kivy 1.9.0
#:import st settings

<Zombie>:
    size: (st.ZOMBIE_SIZE, st.ZOMBIE_SIZE)
    canvas:
        Color:
            rgba: st.ZOMBIE_COLOR

        Ellipse:
            pos: self.pos
            size: self.size

<Man>:
    size: (st.MAN_SIZE, st.MAN_SIZE)
    canvas:
        Color:
            rgba: st.MAN_COLOR

        Ellipse:
            pos: self.pos
            size: self.size
